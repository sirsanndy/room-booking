{
  "title": "Meeting Room Booking - HTTP & Cache Metrics",
  "tags": [
    "http",
    "endpoints",
    "metrics",
    "cache"
  ],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "10s",
  "annotations": {
    "list": []
  },
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "id": 17,
      "title": "Cache Hit Rate per Second",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(cache_gets_total{result=\"hit\"}[1m])) by (cache)",
          "legendFormat": "{{cache}} - Hits/s",
          "refId": "A"
        },
        {
          "expr": "sum(rate(cache_gets_total{result=\"miss\"}[1m])) by (cache)",
          "legendFormat": "{{cache}} - Misses/s",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "ops"
        }
      }
    },
    {
      "datasource": "Prometheus",
      "id": 18,
      "title": "Cache Hit Rate %",
      "type": "gauge",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "(sum(rate(cache_gets_total{result=\"hit\"}[5m])) / sum(rate(cache_gets_total[5m]))) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "green",
                "value": 80
              }
            ]
          },
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "datasource": "Prometheus",
      "id": 19,
      "title": "Cache Operations by Type",
      "type": "stat",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(cache_gets_total{result=\"hit\"}[5m]))",
          "legendFormat": "Hits/s",
          "refId": "A"
        },
        {
          "expr": "sum(rate(cache_gets_total{result=\"miss\"}[5m]))",
          "legendFormat": "Misses/s",
          "refId": "B"
        },
        {
          "expr": "sum(rate(cache_puts_total[5m]))",
          "legendFormat": "Puts/s",
          "refId": "C"
        },
        {
          "expr": "sum(rate(cache_evictions_total[5m]))",
          "legendFormat": "Evictions/s",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "datasource": "Prometheus",
      "id": 20,
      "title": "Cache Hit Rate by Cache Name",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "(sum(rate(cache_gets_total{result=\"hit\"}[1m])) by (cache) / sum(rate(cache_gets_total[1m])) by (cache)) * 100",
          "legendFormat": "{{cache}} - Hit Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "datasource": "Prometheus",
      "id": 21,
      "title": "Cache Size (Entries)",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "cache_size",
          "legendFormat": "{{cache}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "datasource": "Prometheus",
      "id": 22,
      "title": "Cache Statistics Table",
      "type": "table",
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(cache_gets_total{result=\"hit\"}[5m])) by (cache)",
          "format": "table",
          "instant": true,
          "legendFormat": "Hits",
          "refId": "A"
        },
        {
          "expr": "sum(rate(cache_gets_total{result=\"miss\"}[5m])) by (cache)",
          "format": "table",
          "instant": true,
          "legendFormat": "Misses",
          "refId": "B"
        },
        {
          "expr": "(sum(rate(cache_gets_total{result=\"hit\"}[5m])) by (cache) / sum(rate(cache_gets_total[5m])) by (cache)) * 100",
          "format": "table",
          "instant": true,
          "legendFormat": "Hit Rate %",
          "refId": "C"
        }
      ],
      "transformations": [
        {
          "id": "merge"
        },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "cache": "Cache Name",
              "Value #A": "Hits/s",
              "Value #B": "Misses/s",
              "Value #C": "Hit Rate %"
            }
          }
        }
      ]
    },
