{
  "dashboard": {
    "title": "Meeting Room Booking - Detailed HTTP Metrics",
    "tags": ["http", "endpoints", "metrics"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests by Status Code",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[1m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 2,
        "title": "Success vs Errors",
        "type": "piechart",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"2..\"}[5m]))",
            "legendFormat": "Success (2xx)",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{status=~\"4..\"}[5m]))",
            "legendFormat": "Client Error (4xx)",
            "refId": "B"
          },
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m]))",
            "legendFormat": "Server Error (5xx)",
            "refId": "C"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate %",
        "type": "gauge",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 8 },
        "targets": [
          {
            "expr": "(sum(rate(http_requests_total{status=~\"[45]..\"}[5m])) / sum(rate(http_requests_total[5m]))) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 5 },
                { "color": "red", "value": 10 }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Requests per Endpoint",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(endpoint_requests_total[1m])) by (endpoint, method)",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 5,
        "title": "HTTP 200 OK",
        "type": "stat",
        "gridPos": { "x": 0, "y": 16, "w": 4, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_200_ok[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "HTTP 201 Created",
        "type": "stat",
        "gridPos": { "x": 4, "y": 16, "w": 4, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_201_created[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "green", "value": null }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "HTTP 400 Bad Request",
        "type": "stat",
        "gridPos": { "x": 8, "y": 16, "w": 4, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_400_bad_request[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "yellow", "value": null }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "HTTP 401 Unauthorized",
        "type": "stat",
        "gridPos": { "x": 12, "y": 16, "w": 4, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_401_unauthorized[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "orange", "value": null }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "HTTP 429 Rate Limited",
        "type": "stat",
        "gridPos": { "x": 16, "y": 16, "w": 4, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_429_too_many_requests[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "red", "value": null }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "HTTP 500 Internal Error",
        "type": "stat",
        "gridPos": { "x": 20, "y": 16, "w": 4, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_500_internal_error[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "steps": [
                { "color": "red", "value": null }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Response Time by Endpoint (p95)",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[1m])) by (endpoint, le))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 12,
        "title": "Response Time by Status Code",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 20, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[1m])) by (status, le))",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 13,
        "title": "Endpoint Error Details",
        "type": "table",
        "gridPos": { "x": 0, "y": 28, "w": 24, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(http_errors_total[5m])) by (endpoint, status, error_type)",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "endpoint": "Endpoint",
                "status": "Status Code",
                "error_type": "Error Type",
                "Value": "Rate (req/s)"
              }
            }
          }
        ]
      },
      {
        "id": 14,
        "title": "Auth Endpoints - Success Rate",
        "type": "timeseries",
        "gridPos": { "x": 0, "y": 36, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{endpoint=~\"/auth/.*\", status=~\"2..\"}[1m])) by (endpoint)",
            "legendFormat": "{{endpoint}} - Success",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{endpoint=~\"/auth/.*\", status=~\"[45]..\"}[1m])) by (endpoint)",
            "legendFormat": "{{endpoint}} - Error",
            "refId": "B"
          }
        ]
      },
      {
        "id": 15,
        "title": "Booking Endpoints - Success Rate",
        "type": "timeseries",
        "gridPos": { "x": 12, "y": 36, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{endpoint=~\"/bookings.*\", status=~\"2..\"}[1m])) by (endpoint, method)",
            "legendFormat": "{{method}} {{endpoint}} - Success",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{endpoint=~\"/bookings.*\", status=~\"[45]..\"}[1m])) by (endpoint, method)",
            "legendFormat": "{{method}} {{endpoint}} - Error",
            "refId": "B"
          }
        ]
      },
      {
        "id": 16,
        "title": "All HTTP Status Codes",
        "type": "table",
        "gridPos": { "x": 0, "y": 44, "w": 24, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (method, endpoint, status, status_category)",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "method": "Method",
                "endpoint": "Endpoint",
                "status": "Status",
                "status_category": "Category",
                "Value": "Rate (req/s)"
              }
            }
          }
        ]
      }
    ]
  }
}
